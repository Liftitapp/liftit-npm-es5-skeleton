{
  "name": "liftit-npm-es5-skeleton",
  "version": "0.0.1",
  "description": "liftit-npm-es5-skeleton",
  "main": "./dist/index.js",
  "scripts": {
    "start": "nodemon $2 --exec babel-node",
    "debug": "nodemon $2 --exec babel-node --inspect",
    "benchmark": "babel-node benchmark",
    "benchmark:watch": "nodemon $2 --exec babel-node benchmark",
    "lint": "eslint src",
    "lint:fix": "eslint --fix src",
    "build": "babel -s true src -d dist",
    "test": "mocha -c -S -R spec 'tests' --compilers js:babel-register --check-leaks",
    "test:watch": "mocha -w -c -S -R spec 'tests' --compilers js:babel-register --check-leaks",
    "test:reporter-list": "mocha -c -S -R list 'tests' --compilers js:babel-register --check-leaks",
    "test:export": "mocha -S -R mochawesome 'tests' --compilers js:babel-register --reporter-options reportDir=reports --check-leaks",
    "scan:security": "nsp check",
    "scan:updates": "ncu",
    "precommit": "npm run lint",
    "prepush": "npm test",
    "postmerge": "npm install",
    "postrewrite": "npm install"
  },
  "keywords": ["liftit", "boilerplate", "babel", "test"],
  "author": "Angel Celis Botto <angel@lftit.co>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Liftitapp/liftit-npm-es5-skeleton.git"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-node-debug": "^2.0.0",
    "babel-preset-babili": "0.1.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.24.1",
    "babili": "0.1.4",
    "benchmark": "^2.1.4",
    "chai": "^4.1.0",
    "eslint": "^4.3.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "husky": "^0.14.3",
    "microtime": "^2.1.6",
    "mocha": "^3.4.2",
    "mochawesome": "^2.3.0",
    "nodemon": "^1.11.0",
    "npm-check-updates": "^2.12.1",
    "nsp": "^2.6.3"
  },
  "license": "MIT"
}
