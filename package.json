{
  "name": "liftit-npm-es5-skeleton",
  "version": "0.0.1",
  "description": "liftit-npm-es5-skeleton",
  "main": "./dist/index.js",
  "runkitExampleFilename": "example.js",
  "babel": {
    "presets": [
      "stage-3",
      "latest"
    ]
  },
  "scripts": {
    "start": "nodemon $2 --exec babel-node",
    "debug": "nodemon $2 --exec babel-node --inspect",
    "lint": "eslint src",
    "lint:fix": "eslint --fix src",
    "build": "rollup -c",
    "test": "mocha -c -S -R spec 'tests' --compilers js:babel-core/register --check-leaks",
    "test:watch": "mocha -w -c -S -R spec 'tests' --compilers js:babel-core/register --check-leaks",
    "test:reporter-list": "mocha -c -S -R list 'tests' --compilers js:babel-core/register --check-leaks",
    "test:export": "mocha -S -R mochawesome 'tests' --compilers js:babel-core/register --reporter-options reportDir=reports --check-leaks",
    "scan:updates": "ncu",
    "precommit": "npm run lint",
    "prepush": "npm test",
    "postmerge": "npm install",
    "postrewrite": "npm install",
    "upload": "npm publish",
    "update:version": "npm version"
  },
  "keywords": [
    "liftit",
    "boilerplate",
    "babel",
    "test"
  ],
  "author": "Angel Celis Botto <angel@lftit.co>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Liftitapp/liftit-npm-es5-skeleton.git"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.40",
    "@babel/core": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-es2016": "^7.0.0-beta.53",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.26.3",
    "babel-eslint": "^7.2.3",
    "babel-preset-babili": "0.1.4",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-preset-stage-3": "^6.17.0",
    "babel-register": "^6.24.1",
    "chai": "^4.1.0",
    "eslint": "^5.6.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "husky": "^0.14.3",
    "microtime": "^2.1.6",
    "mocha": "^5.2.0",
    "mochawesome": "^3.0.3",
    "nodemon": "^1.11.0",
    "npm-check-updates": "^2.14.2",
    "rimraf": "^2.6.1",
    "rollup": "^0.66.0",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-commonjs": "^9.1.8",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-uglify": "^3.0.0"
  },
  "license": "MIT",
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "npm": "^6.4.1"
  }
}
